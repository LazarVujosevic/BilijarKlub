@model List<BilijarKlub.Models.RezervacijaViewModel>
<h2>Pregled rezervacija</h2>
<table class="table table-hover" id="rezervacijeTable">
    <tr>
        <th>Sto</th>
        <th>Pocetak termina</th>
        <th>Kraj termina</th>
    </tr>
    @foreach (var rezervacija in Model)
    {
        <tr>
            <td>@rezervacija.NazivStola</td>
            <td>@rezervacija.PocetakTermina</td>
            <td>@rezervacija.KrajTermina</td>
        </tr>
    }
</table>
<label id="testLabel"></label>

@section scripts{
    <script src="~/Scripts/jquery.signalR-2.4.1.min.js"></script>
    <script src="~/signalr/hubs"></script>
    <script src="~/Scripts/termini.js"></script>
}